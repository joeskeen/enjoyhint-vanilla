{
  "name": "enjoyhint",
  "version": "4.0.1",
  "description": "Web-tool that provides the simplest way to create interactive tutorials and hints.",
  "main": "dist/enjoyhint.js",
  "type": "module",
  "types": "dist/enjoyhint.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "typed-scss-modules lib && tsc -p . && vite build",
    "preview": "vite preview",
    "build:watch": "concurrently -n=tcm,vite,tsc \"typed-scss-modules -w lib\" \"vite build -w\" \"tsc -p . -w\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xbsoftware/enjoyhint.git"
  },
  "keywords": [
    "enjoyhint",
    "hint",
    "xbs",
    "tutorials"
  ],
  "author": "XB Software",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xbsoftware/enjoyhint/issues"
  },
  "homepage": "https://github.com/xbsoftware/enjoyhint",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "sass": "^1.69.7",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0"
  },
  "dependencies": {
    "konva": "^9.3.0"
  }
}
